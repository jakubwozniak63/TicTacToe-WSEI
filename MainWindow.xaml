<Window x:Class="TicTacToe2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TicTacToe2"
        mc:Ignorable="d"
        Icon="/NewFolder1/wsei.png"
        Title="Start Game" Height="350" Width="350">

    <Window.Background>
        <ImageBrush ImageSource="/NewFolder1/wsei.png">
            
        </ImageBrush>
    </Window.Background>


    <Window.Resources>
        <Style TargetType="Button">

            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="FontStyle" Value="Oblique" />
            <Setter Property="FontSize" Value="20" />

        </Style>
    </Window.Resources>



    <Button Name="Start" Height="35" Click="Button_Click" HorizontalAlignment="Center" VerticalAlignment="Bottom" >
        <Image Source="/NewFolder1/start2.png"></Image>
    </Button>

</Window>
